{
  "name": "vuetify-unsplash",
  "author": "Philipp Treupel",
  "description": "Vue dialog component for image search and selection from unsplash.com based on Vuetfiy with high customizability thanks to over 30 properties.",
  "keywords": [
    "vue",
    "vuetify",
    "unsplash",
    "image",
    "picker",
    "free image",
    "dialog",
    "material design"
  ],
  "version": "0.1.3",
  "license": "MIT",
  "main": "dist/vuetify-unsplash.common.js",
  "unpkg": "dist/vuetify-unsplash.umd.min.js",
  "cdn": "dist/vuetify-unsplash.umd.min.js",
  "browser": "dist/vuetify-unsplash.umd.min.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build-demo": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name vuetify-unsplash ./src/components/VUnsplash.vue --mode production",
    "test:unit": "vue-cli-service test:unit --setupTestFrameworkScriptFile=./tests/unit/setup.js",
    "lint": "vue-cli-service lint"
  },
  "files": [
    "src/components",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^3.6.5",
    "vue": "^2.6.11",
    "vue-stack-grid": "^1.2.1",
    "vuetify": "^2.2.11"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@testing-library/jest-dom": "^5.11.0",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-e2e-cypress": "^4.4.6",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-unit-jest": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "flush-promises": "^1.0.2",
    "jest-serializer-vue": "^2.0.2",
    "prettier": "^1.19.1",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "^2.0.6",
    "vue-jest": "^3.0.6",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0"
  }
}
